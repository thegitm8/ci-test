{
  "name": "@gitm8/ci-test",
  "version": "1.3.1",
  "description": "test setup for CI/CD pipeline with travisCI",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "build": "printf '\\033[0;32mBuild complete!\\033[0m\\n'",
    "release": "npm run --silent build && semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegitm8/ci-test.git"
  },
  "keywords": [
    "test",
    "CI",
    "travisCI",
    "CD"
  ],
  "author": "Doc <mail@antlike.tech>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thegitm8/ci-test/issues"
  },
  "homepage": "https://github.com/thegitm8/ci-test#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.2",
    "@semantic-release/git": "^5.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "semantic-release": "^15.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
